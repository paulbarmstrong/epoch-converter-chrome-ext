{
	"name": "Epoch Converter",
	"description": "Click to show ISO8601 timestamps in place of epoch timestamps.",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_title": "Epoch Converter"
	},
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"js": [
			"scripts/content.js"
			],
			"matches": [
			"https://*/*"
			]
		}
	]
 }